{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write --cache './src/**/*.{ts,tsx,js,jsx,md,mdx,json}'",
    "registry:internal:build": "tsx --tsconfig ./tsconfig.scripts.json ./src/scripts/build-registry.mts",
    "registry:build": "shadcn build ./src/__registry__/registry.autogenerated.json"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "pnpm run lint && pnpm run format"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-popover": "^1.1.12",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@react-pdf/renderer": "^4.3.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.487.0",
    "next": "15.3.0",
    "react": "^19.0.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.0.0",
    "react-pdf": "^9.2.1",
    "simple-icons": "^14.12.2",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "shadcn": "^2.5.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.4",
    "typescript": "^5"
  }
}
